{
    "name": "hhhoegsted/laravel-remote",
    "description": "Spatie Tutorial",
    "keywords": [
        "HHHoegsted",
        "laravel",
        "laravel-remote"
    ],
    "homepage": "https://github.com/hhhoegsted/laravel-remote",
    "license": "MIT",
    "authors": [
        {
            "name": "Hans-Henrik HÃ¸gsted",
            "email": "hh@debtia.dk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Hhhoegsted\\LaravelRemote\\": "src",
            "Hhhoegsted\\LaravelRemote\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hhhoegsted\\LaravelRemote\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hhhoegsted\\LaravelRemote\\LaravelRemoteServiceProvider"
            ],
            "aliases": {
                "LaravelRemote": "Hhhoegsted\\LaravelRemote\\LaravelRemoteFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
